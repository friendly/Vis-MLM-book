  journal =      "Journal of the American Statistical Association",
  volume =       "87",
  number =       "418",
  pages =        "425--429",
  month =        jun,
  year =         "1992",
  bibdate =      "Wed Jan 25 08:06:12 MST 2012",
  bibsource =    "http://www.jstor.org/journals/01621459.html;
